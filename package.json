{
  "name": "premove",
  "version": "1.0.0",
  "repository": "lukeed/premove",
  "description": "A tiny (247B) utility to remove items recursively",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Luke Edwards",
    "email": "luke.edwards05@gmail.com",
    "url": "https://lukeed.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "bundt",
    "pretest": "npm run build",
    "test": "uvu -r esm test"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "modes": {
    "default": "src/async.js"
  },
  "keywords": [
    "del",
    "remove",
    "rimraf"
  ],
  "devDependencies": {
    "bundt": "1.0.1",
    "esm": "3.2.25",
    "mk-dirs": "2.0.0",
    "uvu": "0.0.14"
  }
}
